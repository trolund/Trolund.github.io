{
  "name": "Troels-Lund-Personal-Website",
  "version": "1.0.0",
  "scripts": {
    "dev": "npm run buildIndex & next",
    "build": "npm run buildIndex & next build",
    "start": "next start",
    "export": "next build && next export",
    "lint": "next lint",
    "buildIndex": "ts-node search/indexBuilder.ts",
    "find:unused": "next-unused",
    "prepare": "husky install",
    "format": "prettier --write \"**/*.{js,ts,tsx}\""
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.ts,*.jsx": "eslint --cache --fix"
  },
  "dependencies": {
    "@codaworks/react-glow": "^1.0.6",
    "classnames": "^2.5.1",
    "date-fns": "2.10.0",
    "framer-motion": "^6.5.1",
    "gray-matter": "4.0.2",
    "keyword-extractor": "^0.0.23",
    "next": "15.0.1",
    "react": "19.0.0-rc-69d4b800-20241021",
    "react-device-detect": "^2.2.3",
    "react-dom": "19.0.0-rc-69d4b800-20241021",
    "react-icons": "^4.12.0",
    "react-intersection-observer": "^8.33.1",
    "react-markdown": "^9.0.1",
    "react-particles": "^2.9.3",
    "react-responsive": "^9.0.0-beta.5",
    "react-select": "^5.7.0",
    "react-syntax-highlighter": "^15.4.5",
    "rehype-katex": "^7.0.1",
    "rehype-meta": "^4.0.1",
    "rehype-raw": "^7.0.0",
    "remark": "^15.0.1",
    "remark-gfm": "^4.0.0",
    "remark-html": "^16.0.1",
    "remark-math": "^6.0.0",
    "sass": "latest",
    "ts-node": "^10.9.1",
    "tsparticles": "^2.9.3"
  },
  "devDependencies": {
    "@types/node": "latest",
    "@types/react": "npm:types-react@19.0.0-rc.1",
    "@types/react-syntax-highlighter": "^13.5.2",
    "eslint": "^8.5.0",
    "eslint-config-next": "15.0.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "next-unused": "^0.0.6",
    "postcss-preset-env": "^9.5.15",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "latest",
    "typescript": "^5.6.2"
  },
  "next-unused": {
    "alias": {},
    "include": [
      "components",
      "pages",
      "styles",
      "lib",
      "search",
      "utils",
      "types",
      "hooks",
      "constants"
    ],
    "exclude": [],
    "entrypoints": []
  },
  "license": "MIT",
  "engines": {
    "npm": ">=6.0.0",
    "node": ">=10.0.0"
  },
  "overrides": {
    "@types/react": "npm:types-react@19.0.0-rc.1"
  }
}
